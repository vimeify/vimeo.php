{
    "name": "vimeo/vimeo-api",
    "description": "Official PHP library for the Vimeo API.",
    "homepage": "https://github.com/vimeo/vimeo.php",
    "keywords": ["vimeo", "video"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Vimeo",
            "homepage": "http://vimeo.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3.0 || ^8.0",
        "ext-curl": "*",
        "ext-json":"*",
        "ankitpokhrel/tus-php": "^v2.3.0"
    },
    "autoload": {
        "psr-4": {
            "Vimeify\\Vimeo\\": "src/Vimeo"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vimeify\\Vimeo\\": "tests/Vimeo"
        }
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.7",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^4.30"
    },
    "scripts": {
        "coverage": "phpunit --coverage-html reports/",
        "tests": [
            "psalm",
            "phpunit"
        ]
    }
}
